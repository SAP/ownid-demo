{
  "name": "ownid-client-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "ng build --prod",
    "start:demo": "ng serve demo-app --port 4202 --host 0.0.0.0",
    "build:demo": "ng build demo-app --prod",
    "start:demo-screens": "ng serve demo-screens-app --port 4204 --host 0.0.0.0",
    "build:demo-screens": "ng build demo-screens-app --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "docker:start": "yarn run build && yarn run docker:build && yarn run docker:run",
    "docker:build": "docker build -t ownid-client-app:latest .",
    "docker:run": "docker run --rm -it -p 80:80/tcp ownid-client-app:latest",
    "e2e": "ng e2e",
    "format": "eslint . --ext .ts --fix",
    "verify": "yarn test && yarn lint && yarn build && yarn build:demo && yarn start",
    "postinstall": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.3",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/forms": "~10.1.3",
    "@angular/material": "^10.2.2",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@angular/language-service": "~10.1.3",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^22.0.0",
    "husky": "^5.0.0-beta.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "jest": "^26.4.2",
    "jest-preset-angular": "^8.3.1",
    "jest-resolver-tsconfig-paths": "^0.13.0",
    "jest-trx-results-processor": "^2.2.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "typescript": "4.0.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --max-warnings=0"
    ]
  }
}
